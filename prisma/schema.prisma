generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Users {
  id         Int       @id @default(autoincrement())
  first_name String
  last_name  String
  email      String
  gender     String
  ip_address String
  days       Int
  created_at DateTime  @default(now())
  Meetings   Meetings[]
}

model Meetings {
  id       Int     @id @default(autoincrement())
  user_id  Int
  room_id  Int
  start_day Int
  end_day   Int
  created_at DateTime @default(now())
  User     Users   @relation(fields: [user_id], references: [id])
}